
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("HeadPartial")
<body>
    <div class="container-xxl bg-white p-0">
        <!-- Spinner Start -->
        @await Component.InvokeAsync("SpinnerPartial")
        <!-- Spinner End -->
        <!-- Header Start -->
        @await Component.InvokeAsync("NavbarPartial")
        <!-- Header End -->
        <!-- Carousel Start -->
        @await Component.InvokeAsync("SliderPartial")
        <!-- Carousel End -->
        <!-- Booking Start -->
        @await Component.InvokeAsync("BookingPartial")
        <!-- Booking End -->
        <!-- About Start -->
        @await Component.InvokeAsync("AboutPartial")
        <!-- About End -->
        <!-- Room Start -->
        @await Component.InvokeAsync("RoomPartial")
        <!-- Room End -->
        <!-- Video Start -->
        @await Component.InvokeAsync("VideoPartial")
        <!-- Video Start -->
        <!-- Service Start -->
        @await Component.InvokeAsync("ServicePartial")
        <!-- Service End -->
        <!-- Testimonial Start -->
        @await Component.InvokeAsync("TestimonialPartial")
        <!-- Testimonial End -->
        <!-- Team Start -->
        @await Component.InvokeAsync("TeamPartial")
        <!-- Team End -->
        <!-- Newsletter Start -->
        @await Html.PartialAsync("_SubscribePartial")
        @*await Component.InvokeAsync("NewsletterPartial")*@
        <!-- Newsletter Start -->
        <!-- Footer Start -->
        @await Component.InvokeAsync("FooterPartial")
        <!-- Footer End -->
        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>

    <!-- JavaScript Libraries -->
    @await Component.InvokeAsync("ScriptsPartial")

    <!-- Template Javascript -->
    <script src="~/hoteltmp/js/main.js"></script>
</body>

</html>